<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="love2hina.Windows.MAUI.PhotoViewer.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:love2hina.Windows.MAUI.PhotoViewer.Pages"
    xmlns:views="clr-namespace:love2hina.Windows.MAUI.PhotoViewer.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Route="viewer"
        Title="ビューワー"
        ContentTemplate="{DataTemplate pages:ViewerRootPage}" />
    <ShellContent
        Title="About"
        ContentTemplate="{DataTemplate views:AboutPage}" />
</Shell>
